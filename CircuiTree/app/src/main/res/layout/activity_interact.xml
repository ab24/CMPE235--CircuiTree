<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ab.circuitree.InteractActivity">

    <ToggleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New ToggleButton"
        android:id="@+id/lightButton"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="91dp" />

    <ToggleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New ToggleButton"
        android:id="@+id/soundButton"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/lightButton" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Lights"
        android:id="@+id/textView2"
        android:layout_alignTop="@+id/lightButton"
        android:layout_alignBottom="@+id/lightButton"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Sound"
        android:id="@+id/textView3"
        android:layout_alignBottom="@+id/soundButton"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/soundButton"
        android:layout_toStartOf="@+id/soundButton" />

    <MediaController
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mediaController"
        android:layout_below="@+id/soundButton"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="38dp" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LogOut"
        android:id="@+id/logOut"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:checked="false" />

</RelativeLayout>
